{
  "name": "forphp/laravel-blogify",
  "description": "A Blade-first, highly extensible blogging toolkit for Laravel.",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "M B Parvez",
      "email": "mbparvezme@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
    "intervention/image": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Forphp\\Blogify\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Forphp\\Blogify\\BlogifyServiceProvider"
      ],
      "aliases": {
        "Blogify": "Forphp\\Blogify\\Facades\\Blogify"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}